Pipeline{ 
  
  agent any
  
  stages{
  
  stage("build"){
  
  
    steps{
      echo 'building appliaction...'
       }
  
     }
  
  stage("test"){
     
  
    steps{
         echo 'testing appliaction...'
       }
  
     }

  stage("deploy"){
  
  
    steps{
      echo 'deploying application...'
       }
  
     }

  }
  
  }
  
